/* myc_1_1, 第一個C程式碼 */ 
#include <stdio.h>
#include <stdlib.h>
int main(void)
{
   printf("Hello C!\n");   	/* 印出Hello C! 字串 */
   printf("Hello World!\n");   /* 印出Hello World! 字串 */   
   
#   system("pause");
   return 0;
}


10錯誤：
1.
/* myc_1_1, 第一個C程式碼 */ 
#include <stdio.h>
#include <stdlib.h>
int main(void)
{
   printf("Hello 偉大的恩師!\n");   	/* 印出Hello 偉大的恩師! 字串 */
   printf("Hello Taiwan!\n");   /* 印出Hello Taiwan! 字串 */   
   
#  system("pause");
   return 0;
}

2.
/* myc_1_1, 第一個C程式碼 */ 
#include <stdio.h>
#include <stdlib.h>
int main(void)
{
   printf("Hello 偉大的恩師!\n");   	/* 印出Hello 偉大的恩師! 字串 */
   printf("Hello Taiwan!\n");   /* 印出Hello Taiwan! 字串 */   
   
   system("pause");
*   return 0;
}

3.
/* myc_1_1, 第一個C程式碼 */ 
include <stdio.h>
#include <stdlib.h>
int main(void)
{
   printf("Hello 偉大的恩師!\n");   	/* 印出Hello 偉大的恩師! 字串 */
   printf("Hello Taiwan!\n");   /* 印出Hello Taiwan! 字串 */   
   
   system("pause");
   return 0;
}

4.
/* myc_1_1, 第一個C程式碼 */ 
#include <stdio.h>
#include <stdlib.h>
int main(void)
{
   printf("Hello 偉大的恩師!\n);   	/* 印出Hello 偉大的恩師! 字串 */
   printf("Hello Taiwan!\n");   /* 印出Hello Taiwan! 字串 */   
   
   system("pause");
   return 0;
}

